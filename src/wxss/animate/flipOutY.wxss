@keyframes flipOutY {
  from {
    transform: perspective(800rpx);
  }

  30% {
    transform: perspective(800rpx) rotate3d(0, 1, 0, -15deg);
    opacity: 1;
  }

  to {
    transform: perspective(800rpx) rotate3d(0, 1, 0, 90deg);
    opacity: 0;
  }
}

.ac-flipOutY {
  backface-visibility: visible;
  animation: flipOutY 1s;
}